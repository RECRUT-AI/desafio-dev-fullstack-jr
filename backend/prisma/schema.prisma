// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Pet {
  id          Int     @id @default(autoincrement())
  name        String
  age         Int
  type        PetType
  breed       String
  owner       String
  ownerPhone  String
}

enum PetType {
  CAT
  DOG
}

