{
  "name": "template-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/src/index.js",
    "build": "tsc",
    "dev": "ts-node-dev -r dotenv/config -r tsconfig-paths/register src/index.ts --port=$PORT",
    "migrate:make": "knex migrate:make --knexfile ./src/database/BaseDatabase.ts",
    "migrate:latest": "yarn script:run ./src/database/CreateDatabase.ts && knex migrate:latest --knexfile ./src/database/BaseDatabase.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile ./src/database/BaseDatabase.ts",
    "script:run": "ts-node -r dotenv/config -r tsconfig-paths/register",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/knex": "^0.16.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.23",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "jest": "^29.1.1",
    "ts-jest": "^29.0.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
