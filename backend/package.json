    "express": "^4.18.1",
    "joi": "^17.9.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.3.3",
    "objection": "^3.0.1",
    "swagger-ui-express": "^4.5.0",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.0"
  }
}
